{
  "expo": {
    "name": "Background Image Generator",
    "slug": "BackgroundImageGenerator",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon_app.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5721d4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "bundleIdentifier": "com.ductrung.BackgroundImageGenerator"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_app.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.ductrung.BackgroundImageGenerator"
    },
    "web": {
      "favicon": "./assets/icon_app.jpeg"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Grant the Camera permissions to take the photo from Camera.",
          "photosPermission": "Grant the Photo Library permissions to pick an image from Photo Library."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dfcef6f3-2770-4ef2-9807-0a61457f125a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/dfcef6f3-2770-4ef2-9807-0a61457f125a"
    }
  }
}
